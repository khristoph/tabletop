<h1>Edit Tabletop Game</h1>

<%= simple_form_for @game, html: {multipart: true} do |f| %>
  <%= f.file_field :game_img %>
  <%= f.select :category_id, @categories %>
  <%= f.input :title, label: "Game Title" %>
  <%= f.input :description %>
  <%= f.input :creator %>
  <%= f.button :submit %>
<% end %>